@model IEnumerable<AUSMDM.Models.EventClass>
<center><h1><b>Event</b></h1></center>
<p>
      <a asp-action="CreateEvent"><input type="button" value="Create New" class="btn btn-primary" /></a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdentityNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Protocol)
            </th>
            <th>Actions</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IdentityNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Protocol)
                </td>
                <td>

                    @Html.ActionLink("Delete", "DeleteEvent", new { i = item.IdentityNumber })
                </td>
            </tr>
        }
    </tbody>
</table>
