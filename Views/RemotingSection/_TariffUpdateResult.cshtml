@{
    ViewData["Title"] = "Tariff Update Result";
}
<div class="text-center">
    <h4>Tariff Update Result</h4>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label for="inputDate">Date</label>
            <input type="text" class="form-control" id="inputDate">
        </div>
        <div class="form-group col-md-4">
            <label for="inputTime">Time</label>
            <input type="datetime" name="time" value="" />
        </div>
        <div class="form-group col-md-2">
            <label for="inputMSNO">MSNO</label>
            <input type="text" class="form-control" id="inputCurrent">
        </div>
    </div>
    <div class="container">
        <table class="table table-dark">
            <thead>
                <tr>
                    <th>MSNO</th>
                    <th>Customer Name</th>
                    <th>POC SN</th>
                    <th>Tariff</th>
                    <th>Update Result</th>
                    <th>Update Begin</th>
                    <th>Update End</th>
                    <th>Action</th>

                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>