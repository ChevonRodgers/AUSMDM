<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AUSMDM</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/SideMenu.css" />
</head>
<body>
    <header>
        <div id="mySidebar" class="sidebar">
            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
            <button class="dropdown-btn">
                Report
                <i class="fa fa-file-text" aria-hidden="true"></i>
                <i class="fa fa-caret-down"></i>
            </button>
            <div class="dropdown-container">
                <a href="#">Collection Report <i class="fa fa-file-text" aria-hidden="true"></i> </a>
                <a href="#">Prepay Report <i class="fa fa-file-text" aria-hidden="true"></i> </a>
                <a href="#">Archive Report <i class="fa fa-file-text" aria-hidden="true"></i> </a>
            </div>
            <!--Analysis Tab-->
            <button class="dropdown-btn">
                Analysis
                <i class="fa fa-bar-chart" aria-hidden="true"></i>
                <i class="fa fa-caret-down"></i>
            </button>
            <div class="dropdown-container">
                <a href="#">Event Analysis <i class="fa fa-exclamation-triangle" aria-hidden="true"></i> </a>
                <a href="#">Raw Data <i class="fa fa-microchip" aria-hidden="true"></i> </a>
                <a href="#">Monthly Billing <i class="fa fa-book" aria-hidden="true"></i> </a>
                <a href="#">Load Profile <i class="fa fa-bar-chart" aria-hidden="true"></i> </a>
                <a href="#">Consumption <i class="fa fa-address-card-o" aria-hidden="true"></i> </a>
                <a href="#">Max Demand <i class="fa fa-bar-chart" aria-hidden="true"></i> </a>
                <a href="#">Online Rate <i class="fa fa-arrows-v" aria-hidden="true"></i> </a>
                <a href="#">Summary <i class="fa fa-file-text" aria-hidden="true"></i> </a>
                <a href="#">Task Analysis <i class="fa fa-bar-chart" aria-hidden="true"></i> </a>
                <a href="#">Line Loss <i class="fa fa-exclamation-triangle" aria-hidden="true"></i> </a>
            </div>
            <!--Remoting Tab-->
            <button class="dropdown-btn">
                Remoting
                <i class="fa fa-arrows-h" aria-hidden="true"></i>
                <i class="fa fa-caret-down"></i>
            </button>
            <div class="dropdown-container">
                <a href="#">Realtime Operating <i class="fa fa-clock-o" aria-hidden="true"></i> </a>
                <a href="#">Reading <i class="fa fa-exchange" aria-hidden="true"></i> </a>
                <a href="#">Emergancy Load Control <i class="fa fa-exclamation-triangle" aria-hidden="true"></i> </a>
                <a href="#">Tariff Update Result <i class="fa fa-upload" aria-hidden="true"></i> </a>
            </div>
            <!--Prepay Tab-->
            <button class="dropdown-btn">
                Prepay
                <i class="fa fa-credit-card" aria-hidden="true"></i>
                <i class="fa fa-caret-down"></i>
            </button>
            <div class="dropdown-container">
                <a href="#">Merchant <i class="fa fa-user" aria-hidden="true"></i> </a>
                <a href="#">Debt <i class="fa fa-usd" aria-hidden="true"></i> </a>
                <a href="#">Test Token <i class="fa fa-code-fork" aria-hidden="true"></i> </a>
                <a href="#">Manage Token Log <i class="fa fa-file-text-o" aria-hidden="true"></i> </a>
                <a href="#">Manage Token <i class="fa fa-stack-overflow" aria-hidden="true"></i> </a>
                <a href="#">Transaction <i class="fa fa-exchange" aria-hidden="true"></i> </a>
                <a href="#">Vending <i class="fa fa-desktop" aria-hidden="true"></i> </a>
                <a href="#">My Sales <i class="fa fa-credit-card" aria-hidden="true"></i> </a>
                <a href="#">Special Recharge <i class="fa fa-usd" aria-hidden="true"></i> </a>
                <a href="#">Special Recharge Report <i class="fa fa-file-text" aria-hidden="true"></i> </a>
            </div>
            <!--Archive Tab-->
            <button class="dropdown-btn">
                Archive
                <i class="fa fa-archive" aria-hidden="true"></i>
                <i class="fa fa-caret-down"></i>
            </button>
            <div class="dropdown-container">
                <a href="#">Customer <i class="fa fa-user" aria-hidden="true"></i> </a>
                <a href="#">Meter <i class="fa fa-building-o" aria-hidden="true"></i> </a>
                <a href="#">POC <i class="fa fa-flag" aria-hidden="true"></i> </a>
                <a href="#">Sim Card <i class="fa fa-signal" aria-hidden="true"></i> </a>
                <a href="#">Power Grid <i class="fa fa-bolt" aria-hidden="true"></i> </a>
                <a href="#">Installation Wizard <i class="fa fa-magic" aria-hidden="true"></i> </a>
                <a href="#">Device Upgrade <i class="fa fa-level-up" aria-hidden="true"></i> </a>
                <a href="#">Line Loss <i class="fa fa-exclamation" aria-hidden="true"></i> </a>
            </div>
            <!--Parameter Tab-->
            <button class="dropdown-btn">
                Parameter
                <i class="fa fa-key" aria-hidden="true"></i>
                <i class="fa fa-caret-down"></i>
            </button>
            <div class="dropdown-container">
                <a href="@Url.Action("Supplier", "ParameterSection")" class="elements">Supplier<i class="fa fa-users" aria-hidden="true"></i> </a>
                <a href="@Url.Action("MeterList", "ParameterSection")" class="elements">Meter Type <i class="fa fa-tachometer" aria-hidden="true"></i> </a>
                <a href="@Url.Action("Fee", "ParameterSection")">Fee<i class="fa fa-usd" aria-hidden="true"></i></a>
                <a href="@Url.Action("Tariff", "ParameterSection")">Tariff <i class="fa fa-tag" aria-hidden="true"></i> </a>
                <a href="@Url.Action("SupplyGroup", "ParameterSection")">Supply Group <i class="fa fa-users" aria-hidden="true"></i> </a>
                <a href="@Url.Action("CollectPlan", "ParameterSection")">Collect Plan <i class="fa fa-compress" aria-hidden="true"></i> </a>
                <a href="@Url.Action("Event", "ParameterSection")">Event <i class="fa fa-exclamation-triangle" aria-hidden="true"></i></a>
            </div>
            <!--System Tab-->
            <button class="dropdown-btn">
                System
                <i class="fa fa-cogs" aria-hidden="true"></i>
                <i class="fa fa-caret-down"></i>
            </button>
            <div class="dropdown-container">
                <a href="#">Role <i class="fa fa-user-md" aria-hidden="true"></i> </a>
                <a href="#">User <i class="fa fa-user" aria-hidden="true"></i> </a>
                <a href="#">Notice Management <i class="fa fa-bullhorn" aria-hidden="true"></i> </a>
                <a href="#">Operating Log <i class="fa fa-file-text" aria-hidden="true"></i> </a>
                <a href="#">Login Log <i class="fa fa-file-text" aria-hidden="true"></i> </a>
                <a href="#">My Notice <i class="fa fa-bullhorn" aria-hidden="true"></i> </a>
                <a href="#">Subscribe <i class="fa fa-rss" aria-hidden="true"></i> </a>
            </div>
            </div>


</header>
    <div id="main">
    <button class="openbtn" onclick="openNav()">&#9776;</button>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - AUSMDM - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
